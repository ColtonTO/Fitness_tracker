{{#each posts as |post| }}

<div class="row">
  <div class="col-auto">
    <h2>Welcome, {{username}}!</h2>
  </div>
  <div class="card">
    <h5 class="card-title">{{post.title}}</h5> 
  <div class="card-body">
    <p class="card-text">{{post.post_text}}</p>
    <div class="card">
          <a href="/dashboard" class="btn btn-primary">Comment</a>
    </div>
  </div>
</div>
</div>
{{/each}}